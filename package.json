{
  "name": "legal-case-management",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "audit:components": "node scripts/component-analysis.js",
    "audit:a11y": "node scripts/accessibility-audit.js",
    "audit": "npm run audit:components && npm run audit:a11y",
    "docs:generate": "mkdir -p docs/components && node scripts/generate-component-docs.js",
    "dev": "vite --port 5178",
    "cypress:run": "cypress run --headless --browser chrome",
    "test:e2e:ci": "start-server-and-test dev http-get://localhost:5178 cypress:run",
    "contacts:migrate": "npx tsx scripts/contacts-migration.ts",
    "migration:validate": "npx tsx scripts/migration-validator.ts",
    "contacts:seed": "npm run contacts:migrate seed",
    "contacts:validate": "npm run migration:validate post-validate",
    "verify:tyler": "node scripts/verify-tyler-credentials.js",
    "verify:case-management": "node scripts/verify-case-management-integration.js",
    "verify:efile-schema": "node scripts/verify-efile-schema.js",
    "test:production": "node scripts/production-efile-test.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration --config vitest.integration.config.ts",
    "test:api": "vitest run tests/unit/api",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@refinedev/core": "^4.57.9",
    "@refinedev/react-hook-form": "^4.9.3",
    "@refinedev/react-router-v6": "^4.6.2",
    "@refinedev/simple-rest": "^5.0.10",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.75.7",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.344.0",
    "papaparse": "^5.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.2.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.15.17",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.8.3",
    "chalk": "^4.1.2",
    "cypress": "^14.4.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "glob": "^10.3.10",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.35",
    "shadcn-ui": "^0.9.5",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.1.4",
    "wait-on": "^8.0.3"
  }
}
